{
  "name": "api-form-integrator",
  "version": "1.1.3",
  "description": "API ile entegre çalışan formları yöneten bir paket",
  "author": "İsmail Hallaç",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "browser": "./dist/integrator.cdn.js",
      "types": "./dist/types.d.ts"
    },
    "./bin": {
      "import": "./dist/esm/bin/create-integrator.js",
      "require": "./dist/cjs/bin/create-integrator.cjs"
    }
  },
  "bin": {
    "create-integrator": "./dist/cjs/bin/create-integrator.cjs"
  },
  "files": [
    "dist",
    "bin",
    "types.d.ts",
    "README.md"
  ],
  "scripts": {
    "test": "jest",
    "build:esm": "cross-env NODE_ENV=esm babel src -d dist/esm --extensions .js",
    "build:cjs": "cross-env NODE_ENV=cjs babel src -d dist/cjs --extensions .js --out-file-extension .cjs",
    "build:cdn": "webpack --mode production",
    "build": "rimraf dist && node build.js",
    "prebuild": "rimraf dist bin",
    "postbuild": "cpy 'src/**/*.d.ts' dist --parents",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "sweetalert2": "^11.17.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@types/node": "^20.0.0",
    "babel-loader": "^9.2.1",
    "cpy": "^11.0.0",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.19.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rimraf": "^6.0.1",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "keywords": [
    "api",
    "form",
    "integration",
    "axios",
    "service",
    "cdn",
    "laravel"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RhymeRone/api-form-integrator"
  },
  "bugs": {
    "url": "https://github.com/RhymeRone/api-form-integrator/issues"
  },
  "homepage": "https://github.com/RhymeRone/api-form-integrator#readme"
}
