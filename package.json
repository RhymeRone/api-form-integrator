{
  "name": "api-form-integrator",
  "version": "1.0.2",
  "description": "API ile entegre çalışan formları yöneten bir paket",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "build": "webpack --mode production",
    "prepublish": "npm run build"
  },
  "keywords": [
    "api",
    "form",
    "integration",
    "axios",
    "service"
  ],
  "author": "İsmail Hallaç",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.9",
    "sweetalert2": "^11.17.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "babel-loader": "^9.2.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "setupFiles": [
      "<rootDir>/tests/setup.js"
    ]
  }
